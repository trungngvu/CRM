{
  "name": "battech-crm",
  "version": "1.0.0",
  "private": true,
  "description": "BATTECH CRM",
  "license": "ISC",
  "author": "Shion",
  "scripts": {
    "build": "craco build",
    "eject": "react-scripts eject",
    "lint": "eslint --max-warnings 0 ./src --ext .js,jsx,ts,tsx src",
    "lint-prettier": "prettier --config .prettierrc \"**/*.{html,css,scss,js,jsx,ts,tsx,json,md}\" --list-different",
    "lint-staged-husky": "pretty-quick --staged && lint-staged",
    "lint:fix": "eslint --fix --ext .js,jsx,ts,tsx src",
    "prepare": "husky install",
    "prettier": "prettier --config .prettierrc \"**/*.{html,css,scss,js,jsx,ts,tsx,json,md}\" --write",
    "start": "craco start",
    "test": "craco test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
